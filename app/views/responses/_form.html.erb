<%= simple_form_for(@response) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :participant_id %>
    <%= f.input :item_id %>
    <%= f.input :item_serialized %>
    <%= f.input :value %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
