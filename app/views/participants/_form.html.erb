<%= simple_form_for(@participant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :session %>
    <%= f.input :invite_hash %>
    <%= f.input :participant_hash %>
    <%= f.input :age %>
    <%= f.input :gender %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
